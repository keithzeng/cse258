   "source": [
    "def readGz(f):\n",
    "    for l in tqdm_notebook(gzip.open(f)):\n",
    "        yield eval(l)\n",
    "\n",
    "            \n",
    "data = list(readGz(\"reviews_Electronics_5.json.gz\"))"
